{
   "author": "Tom Hardman",
   "browser_action": {
      "default_icon": "icons/postto512.png",
      "default_popup": "html/postto.html"
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "This extension shares your current page directly to Hacker News. Optionally, you can add a custom title.",
   "icons": {
      "128": "icons/postto128.png",
      "256": "icons/postto256.png",
      "32": "icons/postto32.png",
      "512": "icons/postto512.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Post to Hacker News",
   "permissions": [ "activeTab", "webNavigation" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.2"
}
